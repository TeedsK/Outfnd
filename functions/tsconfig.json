{
    "compilerOptions": {
        "target": "ES2023",
        "module": "ES2022",
        "moduleResolution": "Bundler",
        "outDir": "lib",
        "rootDir": "src",
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "lib": [
            "ES2023"
        ],
        "esModuleInterop": true
    },
    "include": [
        "src"
    ]
}